<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.td3">

    <uses-permission android:name="android.permission.INTERNET"/>


    <application
        android:allowBackup="true"
        android:icon="@mipmap/daft_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/daft_launcher"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".presentation.presentation.view.SplashScreen"
                  android:label="@string/app_name"
                  android:theme="@android:style/Theme.Black.NoTitleBar" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".presentation.presentation.view.MainActivity" />
        <activity android:name=".presentation.presentation.view.GalleryActivity"/>

    </application>
</manifest>